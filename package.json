{
  "name": "@gjsify/docs",
  "version": "0.0.0",
  "description": "JavaScript / TypeScript documentations generated by TypeScript Type Definitions",
  "module": "cli.ts",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "npm run update:submodules",
    "update:submodules": "git submodule update --init --recursive && git submodule foreach git pull origin master",
    "build:types:node-gtk": "npm run clear:types:node-gtk && npm run ts-for-gir -- generate '*' -e node --useNamespace --moduleType commonjs",
    "build:types:gjs": "npm run clear:types:gjs && npm run ts-for-gir -- generate '*' -e gjs --useNamespace --moduleType esm",
    "build:types": "npm run build:types:gjs && npm run build:types:node-gtk",
    "build:docs:node-gtk": "npm run clear:docs:node-gtk && npm run typedoc -- --options ./typedoc.node.js --in ../../@types/node-gtk",
    "build:docs:gjs": "npm run clear:docs:gjs && npm run typedoc -- --options ./typedoc.gjs.js --in ../../@types/Gjs",
    "build:docs:try": "npm run clear:docs:try && npm run typedoc -- --options ./typedoc.try.js",
    "build:docs": "npm run build:docs:gjs && npm run build:docs:node-gtk",
    "build:node-gtk": "npm run build:types:node-gtk && npm run build:docs:node-gtk",
    "build:gjs": "npm run build:types:gjs && npm run build:docs:gjs",
    "build": "npm run build:gjs && npm run build:node-gtk",
    "validate:gjs": "tsc --noEmit ./@types/Gjs/*.ts",
    "validate:node-gtk": "tsc --noEmit ./@types/node-gtk/*.ts",
    "validate": "npm run validate:gjs && npm run validate:node-gtk",
    "clear:docs:node-gtk": "rm -rf ./docs/node-gtk",
    "clear:docs:gjs": "rm -rf ./docs/gjs",
    "clear:docs:try": "rm -rf ./docs/try",
    "clear:docs": "rm -rf ./docs",
    "clear:types:node-gtk": "rm -rf ./@types/node-gtk",
    "clear:types:gjs": "rm -rf ./@types/Gjs",
    "clear": "npm run clear:docs",
    "serve": "http-server ./docs",
    "serve:try": "typedoc-server --doc-dir ./docs/try",
    "link": "npm link ts-for-gir",
    "ts-for-gir": "ts-for-gir",
    "typedoc": "yarn workspace @gjsify/typedoc-custom-cli run start"
  },
  "keywords": [
    "gjs",
    "node.js",
    "typescript",
    "javascript",
    "tsdoc",
    "typedoc",
    "gio",
    "gtk",
    "gnome",
    "gobject-introspection",
    "node-gtk"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "devDependencies": {
    "@ribajs/eslint-config": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/experimental-utils": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.0",
    "prettier": "2.5.1",
    "ts-for-gir": "workspace:*",
    "typescript": "^4.5.5"
  },
  "workspaces": [
    "packages/*",
    "vendor/ts-for-gjs",
    "vendor/riba/packages/*",
    "vendor/riba/backend/*",
    "vendor/riba/infra/*",
    "vendor/riba/vendor/*",
    "!vendor/riba/infra/cli",
    "!vendor/riba/infra/doc",
    "!vendor/riba/infra/schematics"
  ],
  "resolutions": {
    "ts-for-gir": "workspace:*",
    "@ribajs/accessibility": "workspace:*",
    "@ribajs/artcodestudio": "workspace:*",
    "@ribajs/bs4": "workspace:*",
    "@ribajs/bs5": "workspace:*",
    "@ribajs/content-slider": "workspace:*",
    "@ribajs/octobercms": "workspace:*",
    "@ribajs/leaflet-map": "workspace:*",
    "@ribajs/cache": "workspace:*",
    "@ribajs/core": "workspace:*",
    "@ribajs/extras": "workspace:*",
    "@ribajs/events": "workspace:*",
    "@ribajs/history": "workspace:*",
    "@ribajs/i18n": "workspace:*",
    "@ribajs/iconset": "workspace:*",
    "@ribajs/jquery": "workspace:*",
    "@ribajs/moment": "workspace:*",
    "@ribajs/photoswipe": "workspace:*",
    "@ribajs/router": "workspace:*",
    "@ribajs/shopify": "workspace:*",
    "@ribajs/shopify-easdk": "workspace:*",
    "@ribajs/shopify-tda": "workspace:*",
    "@ribajs/utils": "workspace:*",
    "@ribajs/doc": "workspace:*",
    "@ribajs/eslint-config": "workspace:*",
    "@ribajs/types": "workspace:*",
    "@ribajs/webpack-config": "workspace:*",
    "@ribajs/tsconfig": "workspace:*",
    "@ribajs/nest-theme": "workspace:*",
    "@ribajs/npm-package": "workspace:*",
    "@ribajs/babel-config": "workspace:*",
    "@ribajs/strapi": "workspace:*"
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "typedoc-plugin-remote-search": "workspace:^"
  }
}
