{
    "name": "@gjsify/typedoc-plugin-remote-search",
    "version": "0.0.3",
    "main": "dist/index.js",
    "bin": {
        "typedoc-server": "./dist/server/cli.js"
    },
    "homepage": "https://github.com/gjsify/doc/tree/main/packages/typedoc-plugin-remote-search#readme",
    "repository": "https://github.com/gjsify/doc/tree/main/packages/typedoc-plugin-remote-search",
    "author": "Pascal Garber <pascal@artandcode.studio>",
    "license": "AGPL",
    "keywords": [
        "typedoc",
        "typedocplugin",
        "typedoc-plugin",
        "plugin",
        "search",
        "server",
        "serve",
        "remote",
        "lunr"
    ],
    "files": [
        "./dist",
        "./assets"
    ],
    "devDependencies": {
        "@types/koa": "^2",
        "@types/koa__router": "^8.0.11",
        "esbuild": "^0.14.16",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    },
    "peerDependencies": {
        "tslib": "^2.3.0",
        "typedoc": "^0.22.11"
    },
    "scripts": {
        "build": "npm run build:client:prod && npm run build:server",
        "build:server": "tsc",
        "build:client:dev": "node esbuild.client.dev.js",
        "build:client:prod": "node esbuild.client.prod.js",
        "plugin:help": "typedoc --plugin @gjsify/typedoc-plugin-remote-search --help",
        "server:help": "typedoc-server --help",
        "build:example": "typedoc src/index.ts src/types/index.ts src/plugin/plugin.tsx src/server/search-server.ts src/client/main.ts --out ./docs --plugin @gjsify/typedoc-plugin-remote-search",
        "serve:example": "typedoc-server --doc-dir ./docs --limit 0",
        "example": "npm run build:example && npm run serve:example"
    },
    "dependencies": {
        "@koa/router": "^10.1.1",
        "@types/koa-compress": "^4.0.3",
        "@types/koa-static": "^4.0.2",
        "@types/lunr": "^2.3.4",
        "@types/node": "^17.0.12",
        "@types/yargs": "^17.0.8",
        "koa": "^2.13.4",
        "koa-compress": "^5.1.0",
        "koa-static": "^5.0.0",
        "lunr": "^2.3.9",
        "yargs": "^17.3.1"
    }
}
