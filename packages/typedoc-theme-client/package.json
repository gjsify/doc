{
  "name": "@gjsify/typedoc-theme-client",
  "version": "0.0.1",
  "description": "",
  "main": "scripts/index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "concurrently --kill-others-on-fail 'yarn:build:prod' 'yarn:build:try:*'",
    "build:prod": "webpack --env production --config ./webpack.config.cjs",
    "build:dev": "webpack --env development --stats-error-details --config ./webpack.config.cjs",
    "build:try": "concurrently --kill-others-on-fail 'yarn:build:try:*'",
    "build:try:gjs": "webpack --env development --stats-error-details --config ./webpack.try-gjs.cjs",
    "build:try:node-gtk": "webpack --env development --stats-error-details --config ./webpack.try-node-gtk.cjs",
    "watch": "concurrently --kill-others-on-fail 'yarn:watch:dev' 'yarn:watch:try:*'",
    "watch:dev": "yarn run build:dev --watch",
    "watch:prod": "yarn run build:prod --watch",
    "watch:try": "concurrently --kill-others 'yarn:watch:try:*'",
    "watch:try:gjs": "yarn run build:try:gjs --watch",
    "watch:try:node-gtk": "yarn run build:try:node-gtk --watch",
    "lint": "eslint . --ext .ts,.tsx --fix && npm run lint:csr",
    "lint:csr": "tsc -p tsconfig.json --noEmit"
  },
  "homepage": "https://github.com/gjsify/doc/tree/main/packages/typedoc-theme-client#readme",
  "repository": "https://github.com/gjsify/doc/tree/main/packages/typedoc-theme-client",
