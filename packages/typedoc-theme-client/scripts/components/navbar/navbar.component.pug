.container
  .table-wrap
    tsd-search#tsd-search.table-cell.ready(hostname='localhost' port='3024')
    #tsd-widgets.table-cell
      #tsd-filter
        a.tsd-widget.options.no-caption(href='#' data-toggle='options') Options
        .tsd-filter-group
          #tsd-filter-visibility.tsd-select
            span.tsd-select-label All
            ul.tsd-select-list
              li(data-value='public') Public
              li(data-value='protected') Public/Protected
              li.selected(data-value='private') All
          input#tsd-filter-inherited(type='checkbox' checked='{true}')
          label.tsd-widget(for='tsd-filter-inherited') Inherited
          input#tsd-filter-externals(type='checkbox' checked='{true}')
          label.tsd-widget(for='tsd-filter-externals') Externals
      a.tsd-widget.menu.no-caption(href='#' data-toggle='menu') Menu
