{
  "name": "@gjsify/typedoc-custom-cli",
  "version": "0.0.0",
  "description": "JavaScript / TypeScript documentations generated by TypeScript Type Definitions",
  "module": "cli.ts",
  "type": "commonjs",
  "repository": "https://github.com/gjsify/doc/tree/main/packages/typedoc-custom-cli",
  "bin": {
    "gjsify-doc": "./cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --max_old_space_size=25600 -r ts-node/register cli.ts",
    "build": "tsc --project ./tsconfig.json && chmod +x ./cli.js",
    "watch": "tsc --project ./tsconfig.json --watch"
  },
  "keywords": [
    "gjs",
    "node.js",
    "typescript",
    "javascript",
    "tsdoc",
    "typedoc",
    "gio",
    "gtk",
    "gnome",
    "gobject-introspection",
    "node-gtk"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "AGPL",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/minimatch": "^3.0.5",
    "@types/yargs-parser": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@gjsify/typedoc-plugin-remote-search": "workspace:^",
    "@gjsify/typedoc-theme": "workspace:^",
    "@types/node": "^16",
    "@types/yargs": "^17.0.8",
    "app-root-path": "^3.0.0",
    "minimatch": "^3.0.4",
    "source-map-support": "^0.5.21",
    "yargs": "^17.3.1"
  }
}
